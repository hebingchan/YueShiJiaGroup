<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.a1000phone.n5thgroup.yueshijiagroup.view.Banner
            android:id="@+id/banner"
            android:layout_width="match_parent"
            android:layout_height="200dp"></com.a1000phone.n5thgroup.yueshijiagroup.view.Banner>

        <RadioGroup android:id="@+id/dot_group" android:layout_width="match_parent"
                    android:layout_height="20dp" android:layout_gravity="bottom"
                    android:gravity="center" android:orientation="horizontal">

        </RadioGroup>
    </FrameLayout>


</LinearLayout>
